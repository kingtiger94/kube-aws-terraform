    context:
      cluster: kubernetes
      user: kube-proxy
current-context: kube-proxy
users:
  - name: kube-proxy
    user:
      client-certificate: /var/lib/kubernetes/kube-proxy.pem
      client-key: /var/lib/kubernetes/kube-proxy-key.pem
